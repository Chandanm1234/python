def calculate_grade(mark):
    if mark >= 90:#
        return "A+"
    elif mark >= 80:#2
        return "A"
    elif mark >= 70:#3
        return "B"
    elif mark >= 60:#4
        return "C"
    elif mark >= 50:#5
        return "D"
    else:
        return "F"

# Example usage
marks = [78, 92, 60, 45, 85]
for mark in marks:
    grade = calculate_grade(mark)
    print(f"Mark: {mark}, Grade: {grade}")
